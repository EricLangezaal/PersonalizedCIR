#!/bin/bash

#SBATCH --partition=gpu
#SBATCH --gpus=1
#SBATCH --job-name=test_ikat_dense_llama
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=18
#SBATCH --time=00:30:00
#SBATCH --output=outputs/test_ikat_dense_llama_%A.out

# Load necessary modules
module purge
module load 2024
module load Anaconda3/2024.06-1

# Activate the Conda environment
source activate ir2

# Set the input directory
INPUT_PATH="data/results/Lla2023_test_All_0shot_prompt_type1.jsonl"

srun python -u pcir/ance_ikat.py --config=pcir/ance_ikat_config.toml --test_file_path $INPUT_PATH --year "2023"
